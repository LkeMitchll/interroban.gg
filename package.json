{
  "name": "interroban.gg",
  "version": "4.5.0",
  "description": "Personal site, blog, and portfolio of Luke Mitchell, a design director, product designer, and UI developer.",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@octokit/core": "^3.3.0",
    "autoprefixer": "^10.2.4",
    "contentful": "^8.1.7",
    "cssnano": "^5.0.6",
    "eleventy-plugin-reading-time": "^0.0.1",
    "mdast-util-to-hast": "^10.1.1",
    "node-fetch": "^2.6.1",
    "postcss": "^8.2.10",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.0",
    "remark-abbr": "^1.4.0",
    "remark-external-links": "^8.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-html": "^13.0.2",
    "remark-parse": "^9.0.0",
    "remark-slug": "^6.0.0",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "curlylint": "^0.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "clean": "rm -rf _site",
    "build": "npm-run-all clean build:*",
    "build:eleventy": "eleventy",
    "build:postcss": "postcss src/css/index.css -o _site/css/site.css --config ./postcss.config.js",
    "dev": "npm-run-all clean --parallel dev:netlify dev:postcss",
    "dev:netlify": "netlify dev",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss src/css/index.css -o _site/css/site.css --config ./postcss.config.js -w",
    "lint": "npm-run-all lint:*",
    "lint:stylelint": "stylelint ./src/css",
    "lint:eslint": "eslint ./src",
    "lint:html": "curlylint ./src"
  }
}
