{
  "name": "interrobang-11ty",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "autoprefixer": "^10.2.4",
    "contentful": "^8.1.7",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "mdast-util-to-hast": "^10.1.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "prettier": "^2.2.1",
    "remark-footnotes": "^3.0.0",
    "remark-html": "^13.0.1",
    "remark-parse": "^9.0.0",
    "unified": "^9.2.0",
    "unist-util-visit": "^2.0.3"
  },
  "scripts": {
    "build": "npm-run-all -s clean build:*",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:postcss": "postcss assets/style.css -o _site/assets/style.css",
    "clean": "rm -rf _site",
    "dev": "netlify dev",
    "dev:all": "npm-run-all -p dev:eleventy dev:postcss",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss assets/style.css -o _site/assets/style.css --map --watch"
  }
}
